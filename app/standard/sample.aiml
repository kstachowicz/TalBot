<aiml>

<category>
	<pattern>HELLO</pattern>
	<template>
	<random>
		<li>Hi!</li>
		<li>Hey!</li>
		<li>Hello!</li>
		<li>Hello</li>
		<li>Good<random><li> morning</li><li> afternoon</li><li> evening</li><li> night</li></random></li>
		<li>Hello, <get name="master"/>.</>/li>
	</random>
	</template>
</category>

<category>
	<pattern>HELLO *</pattern>
	<template>
	<srai>HELLO</srai>
	</template>
</category>

<category>
	<pattern>HI</pattern>
	<template><srai>HELLO</srai></template>
</category>

<category>
	<pattern>HI*</pattern>
	<template><srai>HELLO</srai></template>
</category>

<category>
	<pattern>HI *</pattern>
	<template>
	<srai>HELLO</srai>
	</template>
</category>

