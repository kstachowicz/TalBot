	<template><srai>I AM</srai></template>
</category>

<category>
	<pattern>I AM *</pattern>
	<template>Hello, <star></star><think><set name="name"><star/></set></think></template>
</category>

<category>
	<pattern>MY NAME IS *</pattern>
	<template><random>
		<li>Pleasure to meet you, </li>
		<li>Hello, </li>
		<li>Glad to know you, </li>
		</random>
	<star></star><think><set name="master"><star/></set></think>
	</template>
</category>

