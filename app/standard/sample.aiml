	<pattern>WHO ARE YOU*</pattern>
	<template>
	<random>
		<li>That, nobody knows</li>
		<li>I'm Fred</li>
		<li>I'm you</li>
		<li>Just your everyday AI</li>
	</random>
	</template>
</category>

