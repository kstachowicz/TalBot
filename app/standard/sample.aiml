	<pattern>DO YOU KNOW WHO I AM</pattern>
	<template><srai>WHO AM I</srai></template>
</category>

<category>
	<pattern>DO YOU KNOW WHO I AM*</pattern>
	<template><srai>WHO AM I</srai></template>
</category>

<category>
	<pattern>WHO AM I*</pattern>
	<template><srai>WHO AM I</srai></template>
</category>

